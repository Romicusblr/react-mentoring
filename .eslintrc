{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true,
    }],
    "react/jsx-filename-extension": [1, { "extensions": [".test.js", ".jsx"] }],
  },
  "parser": "babel-eslint",
  "globals": {
    "shallow": true,
    "React": true,
}
}
